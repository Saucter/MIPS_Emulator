{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Debug eMIPS Manually Compiled",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/eMIPS",
        "args": ["tests/asm_tier1/arith", "100000"],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "externalConsole": true,
        "MIMode": "gdb",
        "miDebuggerPath": "C:/msys64/usr/bin/gdb.exe",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ]
      }
    ]
  }
  