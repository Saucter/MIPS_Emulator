{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build eMIPS",
      "type": "shell",
      "command": "C:\\Windows\\System32\\wsl.exe",
      "args": ["make"],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": ["$gcc"]
    },
    {
      "label": "clean",
      "type": "shell",
      "command": "C:\\Windows\\System32\\wsl.exe",
      "args": ["make", "clean"],
      "options": {
        "cwd": "${workspaceFolder}"
      }
    }
  ]
}
